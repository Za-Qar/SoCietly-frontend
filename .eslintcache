le from \"./signIn.module.css\";\r\nimport cn from \"classnames\";\r\nimport SignInSVG from \"./background\";\r\n\r\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\r\nimport { faGoogle } from \"@fortawesome/free-brands-svg-icons\";\r\n\r\nexport default function UserSignIn() {\r\n  const [authUser, loading, error] = useAuthContext();\r\n\r\n  function handleGoogle() {\r\n    signInWithGoogle();\r\n  }\r\n\r\n  if (loading) {\r\n    return <Loading />;\r\n  }\r\n\r\n  return authUser ? (\r\n    <Redirect to={\"/\"}></Redirect>\r\n  ) : (\r\n    <div className={cn(style.backgroundImage)}>\r\n      <div className={cn(style.row)}>\r\n        <div className={cn(style.column)}>\r\n          <div className={cn(style.container, style.moveBehind)}>\r\n            <SignInSVG />\r\n          </div>\r\n        </div>\r\n        <div className={cn(style.column)}>\r\n          <div\r\n            className={cn(\r\n              style.container,\r\n              style.shadowContainer,\r\n              style.marginLeft\r\n            )}\r\n          >\r\n            <div className={cn(style.content)}>\r\n              <h1 className={cn(style.logoText)}> SoCietly</h1>\r\n              <h3 className={cn(style.logoSubText)}> by SoC</h3>\r\n              <br />\r\n              <button onClick={handleGoogle} className={cn(style.buttonGoogle)}>\r\n                <FontAwesomeIcon\r\n                  icon={faGoogle}\r\n                  style={{ marginRight: \"7px\" }}\r\n                />{\" \"}\r\n                Continue with Google\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\W Project Month\\code\\frontend - falcon5ive\\src\\Pages\\Alumni\\Alumni.js",["347","348"],"//React\r\nimport React, { useState, useEffect } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\n//Config\r\nimport { url 