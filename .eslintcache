ttern=\"https://.*\"\r\n                    as={\r\n                      <TextField id=\"portfolio\" label=\"Portfolio\" fullWidth />\r\n                    }\r\n                    control={control}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              <Grid item xs={12}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"other\"\r\n                    placeholder=\"https://example.com\"\r\n                    pattern=\"https://.*\"\r\n                    as={<TextField id=\"other\" label=\"Other\" fullWidth />}\r\n                    control={control}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n            </Grid>\r\n          </React.Fragment>\r\n\r\n          <div className=\"signupSubmit\">\r\n            <div className=\"signupSubmitAligner\">\r\n              {/* Submit form button */}\r\n              <button\r\n                onClick={logout}\r\n                className=\"button-cancel buttonMarginRight\"\r\n              >\r\n                Return to Login\r\n              </button>\r\n              <input\r\n                type=\"submit\"\r\n                value=\"Next\"\r\n                className=\"button buttonMarginLeft\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  ) : (\r\n    <Redirect to={\"/login\"}></Redirect>\r\n  );\r\n}\r\n","C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\W Project Month\\code\\frontend - falcon5ive\\src\\Components\\NavBar\\nav.js",["360","361","362","363"],"import React, { useState } from \"react\";\r\n\r\n//images\r\nimport soc from \"../../Images/soc.png\";\r\nimport logo from \"../../Images/logo.png\";\r\n\r\n//components\r\nimport { logout } from \"../../Components/Firebase/auth\";\r\n\r\n//Context\r\nimport { useUserContext } from \"../../Context/userContext\