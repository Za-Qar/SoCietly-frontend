ct-google-maps/api\";\r\n\r\nimport \"./maps.css\";\r\n\r\nconst libraries = [\"places\"];\r\nconst mapContainerStyle = {\r\n  height: \"100vh\",\r\n  width: \"100vw\",\r\n};\r\n\r\nconst center = {\r\n  lat: 52.47653701744309,\r\n  lng: -1.890981567759743,\r\n};\r\n\r\nconst options = {\r\n  zoomControl: true,\r\n};\r\n\r\nfunction Maps({ marker, setMarker, isEditing }) {\r\n  const { isLoaded, loadError } = useLoadScript({\r\n    // googleMapsApiKey: \"AIzaSyBn62Gatuw8cbCB2LUcuNGv1mVGgakvh4Y\",\r\n    libraries,\r\n  });\r\n\r\n  function onMapClick(e) {\r\n    console.log({\r\n      lat: e.latLng.lat(),\r\n      lng: e.latLng.lng(),\r\n    });\r\n    setMarker({\r\n      lat: e.latLng.lat(),\r\n      lng: e.latLng.lng(),\r\n    });\r\n  }\r\n\r\n  const mapRef = useRef();\r\n  const onMapLoad = useCallback((map) => {\r\n    mapRef.current = map;\r\n  }, []);\r\n\r\n  if (loadError) return \"Error\";\r\n  if (!isLoaded) return \"Loading...\";\r\n\r\n  return (\r\n    <div>\r\n      <GoogleMap\r\n        id=\"map\"\r\n        mapContainerStyle={mapContainerStyle}\r\n        zoom={13}\r\n        center={isEditing ? center : marker}\r\n        options={options}\r\n        onClick={(e) => {\r\n          isEditing && onMapClick(e);\r\n        }}\r\n        onLoad={onMapLoad}\r\n      >\r\n        {marker && (\r\n          <Marker\r\n            key={`${marker.lat}-${marker.lng}`}\r\n            position={marker}\r\n            icon={{\r\n              //we can add a url: \"smth\"; to change the location style\r\n              origin: new window.google.maps.Point(0, 0),\r\n              anchor: new window.google.maps.Point(15, 15),\r\n              scaledSize: new window.google.maps.Size(30, 30),\r\n            }}\r\n          />\r\n        )}\r\n      </GoogleMap>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Maps;\r\n","C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\W Project Month\\code\\frontend - falcon5ive\\src\\Components\\userData.js",[],"C