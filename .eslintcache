le\"\r\n                    as={\r\n                      <TextField\r\n                        id=\"currentRole\"\r\n                        label=\"Current Role\"\r\n                        fullWidth\r\n                        required\r\n                      />\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              {/*----------Current Employer----------*/}\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"currentEmployer\"\r\n                    as={\r\n                      <TextField\r\n                        id=\"currentEmployer\"\r\n                        label=\"Current Employer\"\r\n                        fullWidth\r\n                        required\r\n                      />\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              {/*----------Skills Input----------*/}\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl\r\n                  variant=\"outlined\"\r\n                  fullWidth\r\n                  onChange={(e) => setSkillInput(e.target.value)}\r\n                  value={skillInput}\r\n                >\r\n                  <Controller\r\n                    name=\"skills\"\r\n                    as={<TextField id=\"skills\" label=\"skills\" required />}\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                  <div className=\"addSkillButtonAligner\">\r\n                    <button onClick={(e) => addToSkills(e)} className=\"button\">\r\n                      Add Skill