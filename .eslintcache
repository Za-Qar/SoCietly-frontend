n\" },\r\n    })\r\n      .then((res) => res.json())\r\n      .then((data) => console.log(\"this is the user data: \", data))\r\n      .catch((error) => console.log(\"user creation error error: \", error));\r\n\r\n    setComplete(true);\r\n  };\r\n\r\n  function consoleLog() {\r\n    console.log(user);\r\n    console.log(marker);\r\n  }\r\n\r\n  if (!complete) {\r\n    return (\r\n      <div>\r\n        <button onClick={consoleLog}>Get User</button>\r\n        <form onSubmit={handleSubmit(createEvent)}>\r\n          <span>\r\n            <p>Event Name:</p>\r\n            <input name=\"eventName\" ref={register} required />\r\n          </span>\r\n          <span>\r\n            <p>Event Type:</p>\r\n            <select id=\"eventTypes\" name=\"eventTypes\" ref={register}>\r\n              <option value=\"education\">Education</option>\r\n              <option value=\"social\">Social</option>\r\n              <option value=\"community\">Community</option>\r\n            </select>\r\n          </span>\r\n          <span>\r\n            <p>Date:</p>\r\n            <input name=\"date\" type=\"date\" ref={register} required />\r\n          </span>\r\n          <span>\r\n            <p>Time:</p>\r\n            <input name=\"time\" type=\"time\" ref={register} required />\r\n          </span>\r\n          <span>\r\n            <p>Description:</p>\r\n            <textarea\r\n              name=\"description\"\r\n              rows=\"10\"\r\n              cols=\"30\"\r\n              ref={register}\r\n            ></textarea>\r\n          </span>\r\n          <span>\r\n            <p>Image:</p>\r\n            <input name=\"image\" ref={register} required />\r\n          </span>\r\n          <span>\r\n            <p>Location:</p>\r\n            <div>\r\n              <Maps marker={marker} setMarker={setMarker} isEditing />\r\n              <button onClick={consoleLog}>Console.log</button>\r\n            </div>\r\n          </span>\r\n          <span>\r\n            <p>Volunteers:</p>\r\n            <select id=\"eventVolunteers\" name=\"eventVolunteers\" ref={register}>\r\n              <option value=\"true\">Yes</option>\r\n              <option value=\"false\">No</option>\r\n            </select>\r\n          </span>\r\n          <input type=\"submit\" />\r\n        </form>\r\n      </div>\r\n    );\r\n  } else if (complete) {\r\n    return (\r\n      <div>\r\n        <p>Completed confirmation design comes here</p>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default CreateEvent;\r\n\r\n// onClick={(e) => {\r\n//     e.preventDefault();\r\n//     //take the e.preventDefault(); away when it comes time for release\r\n//   }}\r\n","C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\W Project Month\\code\\frontend - falcon5ive\\src\\Pages\\Events\\events.js",["209","210","211","212","213"],"import React, { useState, useEffect } from \"react\";\r\nimport { v4 as uuidv4 } from \"uuid\";\r\n\r\nimport Event from \"../../Components/Event/event.js\";\r\n\r\n//styling\r\nimport style from \"./events.module.css\";\r\n\r\nfunction GetAllEvents() {\r\n  const [allEvents, setAllEvents] = useState([]);\r\n  const [attendingList, setAttendingList] = useState([]);\r\n\r\n  /*---------------Increment Counter backend----------------*/\r\n  let addToAttend = (id, arr) => {\r\n    console.log(id, arr);\r\n    fetch(`https://falcon5ives.herokuapp.com/events/${id}`, {\r\n      method: \"PATCH\",\r\n      body: JSON.stringify({ attendingList: arr }),\r\n      headers: { \"Content-Type\": \"application/json\" },\r\n    })\r\n      .then((res) => res.json())\r\n      .then((data) => console.log(data))\r\n      .catch((error) => console.log(error));\r\n  };\r\n\r\n  async function get() {\r\n    let res = await fetch(\"https://falcon5ives.herokuapp.com/events\");\r\n    let data = await res.json();\r\n    console.log(data);\r\n    setAllEvents(data.payload);\r\n  }\r\n\r\n  useEffect(() => {\r\n    get();\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    setInterval(() => {\r\n      get();\r\n    }, 300000);\r\n  }, []);\r\n\r\n  return (\r\n    <div>\r\n      <button onClick={() => console.log(allEvents)}>log</button>\r\n      <p>Get All Events</p>\r\n      <button>Create Event</button>\r\n      <button>My Events</button>\r\n\r\n      <section>\r\n        <h3>Education</h3>\r\n        {allEvents.map((item, index) => {\r\n          if (item.eventtype === \"education\") {\r\n            let date = new Date(item.date).toDateString();\r\n            return (\r\n              <Event\r\n                key={uuidv4()}\r\n                date={date}\r\n                setAttending={setAttendingList}\r\n                addToAttend={addToAttend}\r\n                item={item}\r\n              />\r\n            );\r\n          }\r\n        })}\r\n      </section>\r\n\r\n      <section>\r\n        <h3>Social</h3>\r\n        {allEvents.map((item, index) => {\r\n          if (item.eventtype === \"social\") {\r\n            let date = new Date(item.date).toDateString();\r\n            return (\r\n              <Event\r\n                key={uuidv4()}\r\n                date={date}\r\n                setAttending={setAttendingList}\r\n                addToAttend={addToAttend}\r\n                item={item}\r\n              />\r\n            );\r\n          }\r\n        })}\r\n      </section>\r\n\r\n      <section>\r\n        <h3>Community</h3>\r\n        {allEvents.map((item, index) => {\r\n          if (item.eventtype === \"community\") {\r\n            let date = new Date(item.date).toDateString();\r\n            return (\r\n              <Event\r\n                key={uuidv4()}\r\n                date={date}\r\n                setAttending={setAttendingList}\r\n                addToAttend={addToAttend}\r\n                item={item}\r\n              />\r\n            );\r\n          }\r\n        })}\r\n      </section>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default GetAllEvents;\r\n","C:\\Users\\herzallr\\Desktop