 => console.log(\"this is the image upload data: \", data))\r\n  //     .catch((error) => console.log(\"image upload error: \", error));\r\n  // };\r\n\r\n  return (\r\n    <div>\r\n      <input type=\"file\" onChange={fileInput} value={selectedFile} />\r\n\r\n      {previewSource && (\r\n        <img\r\n          src={previewSource}\r\n          alt=\"your uploaded image\"\r\n          className={style.img}\r\n        />\r\n      )}\r\n\r\n      {!uploaded && (\r\n        <button onClick={submitImage} className=\"button maxWidth\">\r\n          Upload Image\r\n        </button>\r\n      )}\r\n\r\n      {uploaded && (\r\n        <button\r\n          onClick={submitImage}\r\n          className={cn(style.buttonComplete, \"maxWidth\")}\r\n        >\r\n          <i class=\"fas fa-check\"></i> Upload Successful\r\n        </button>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\n// import React, { useState } from \"react\";\r\n// const axios = require(\"axios\");\r\n\r\n// function ReactUploadImage() {\r\n//   const [image, setImage] = useState({ file: null });\r\n\r\n//   function onFormSubmit(e) {\r\n//     e.preventDefault();\r\n//     const formData = new FormData();\r\n//     formData.append(\"myImage\", image.file);\r\n\r\n//     console.log(formData);\r\n//     console.log(image);\r\n\r\n//     const config = {\r\n//       headers: {\r\n//         \"content-type\": \"multipart/form-data\",\r\n//       },\r\n//     };\r\n//     axios\r\n//       .post(\r\n//         \"https://falcon5ives.herokuapp.com/users/upload\",\r\n//         formData,\r\n//         config,\r\n//         console.log(config)\r\n//       )\r\n//       .then((response) => {\r\n//         alert(\"The file is successfully uploaded\", console.log(response));\r\n//       })\r\n//       .catch((error) => {\r\n//         console.log(error);\r\n//       });\r\n//   }\r\n\r\n//   function onChange(e) {\r\n//     console.log(\"e.target.value\", e.target.value);\r\n//     setImage(e.target.files[0].name);\r