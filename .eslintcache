)\n      .then((data) => console.log(data))\n      .catch((error) => console.log(error));\n  };\n\n  async function get() {\n    let res = await fetch(`${url}/events`);\n    let data = await res.json();\n    console.log(data);\n    setAllEvents(data.payload);\n  }\n\n  useEffect(() => {\n    get();\n  }, []);\n\n  useEffect(() => {\n    setInterval(() => {\n      get();\n    }, 300000);\n  }, []);\n\n  return (\n    <div>\n      {user && (\n        <div className={style.row}>\n          <UserLeftSide />\n          <div className=\"container\">\n            <div className=\"column1\">\n              <section className=\"columnTwo\">\n                <div className=\"welcome\">\n                  <h3>Hello {user?.username}</h3>\n                  <h4>Here are the current planned events</h4>\n                </div>\n                <div className={style.buttons}>\n                  <Link to=\"/createevent\">\n                    <button className=\"button\">Create Event</button>\n                  </Link>\n                  <Link to=\"/myevents\">\n                    <button className=\"button\">My Events</button>\n                  </Link>\n                </div>\n\n                <div>\n                  <label for=\"filter\">Filter by Cohort</label>\n                  <select\n                    name=\"filter\"\n                    onChange={(e) => {\n                      if (e.target.value === \"all\") {\n                        setFilterValue(null);\n                        return;\n                      }\n                      setFilterValue(parseInt(e.target.value));\n                    }}\n                  >\n                    <option selected value={\"all\"}>\n                      All\n                    </option>\n                    {getEventType(allEvents).map((event) => {\n                      return <option value={event}>{event}</option>;\n                    })}\n                  </select>\n                </div>\n\n                <section className=\"base\">\n                  <h3>Education</h3>\n                  <div>\n                    <Grid container spacing={3}>\n                      {allEvents.map((item, index) => {\n                        if (item.eventtype === \"education\") {\n                          let date = new Date(item.date).toDateString();\n                          return (\n                            <Grid item xs={3}>\n                              <Paper>\n                                <Card\n                                  key={uuidv4()}\n                                  date={date}\n                                  setAttending={setAttendingList}\n                                  addToAttend={addToAttend}\n                                  item={item}\n                                />\n                              </Paper>\n                            </Grid>\n                          );\n                        }\n                      })}\n                    </Grid>\n                  </div>\n                </section>\n\n                <section className=\"base\">\n                  <h3>Social</h3>\n\n                  <Grid container spacing={3}>\n                    {allEvents.map((item, index) => {\n                      if (item.eventtype === \"social\") {\n                        let date = new Date(item.date).toDateString();\n                        return (\n                          <Grid item xs={3}>\n                            <Paper>\n                              <Card\n                                key={uuidv4()}\n                                date={date}\n                                setAttending={setAttendingList}\n                                addToAttend={addToAttend}\n                                item={item}\n                              />\n                            </Paper>\n                          </Grid>\n                        );\n                      }\n                    })}\n                  </Grid>\n        